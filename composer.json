{
    "name": "sztwiorok/wp-workshop",
    "description": "Base for WordPress projects",
    "type": "project",
    "authors": [
        {
            "name": "Rafa≈Ç Sztwiorok",
            "email": "rafal.sztwiorok@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "wp-content/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    },
    "require": {
        "wpackagist-theme/understrap":"0.2.9"
    }
}
